@base <http://example.org/mapping/> .
@prefix rr:   <http://www.w3.org/ns/r2rml#> .          # R2RML mapping vocabulary
@prefix rml:  <http://semweb.mmlab.be/ns/rml#> .         # RML extension for heterogeneous sources
@prefix ql:   <http://semweb.mmlab.be/ns/ql#> .           # Query language; here we use CSV
@prefix parsec: <https://hecate.ia.forth.gr#> .          # PARSEC ontology namespace (blueprint source)
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .  # RDF Schema (for labels, etc.)
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .       # XML Schema (for datatypes)


<#HecateCSV> a rml:LogicalSource ;
    rml:source "CURRENT_CSV" ;
    rml:referenceFormulation ql:CSV .


################################################################################
# Galaxy Mapping: Create a Galaxy entity (E3_Galaxy)
################################################################################
<#GalaxyMapping> a rr:TriplesMap ;
    rml:logicalSource <#HecateCSV> ;

    # Generate a unique URI for each galaxy using the PGC column.
    rr:subjectMap [
        rr:template "https://hecate.ia.forth.gr/galaxy/{PGC}" ;
        rr:class parsec:E3_Galaxy ;
    ] ;
    
    # Instead of directly using P12_has_identifier, we link to an identifier entity.
    rr:predicateObjectMap [
        rr:predicate parsec:P33_provides_identifier ;
        rr:objectMap [ rr:parentTriplesMap <#GalaxyIdentifier> ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P44_is_identified_by ;
        rr:objectMap [ rr:parentTriplesMap <#GalaxyIdentifier> ] ;
    ] ;

    # Map the OBJNAME column to rdfs:label for a human-readable name.
    rr:predicateObjectMap [
        rr:predicate rdfs:label ;
        rr:objectMap [ rml:reference "OBJNAME" ] ;
    ] ;



    ################################################################################
    # Linking the Galaxy to its Measurements
    ################################################################################

    # RA & DEC & D Measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementRA> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementDEC> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementD> ] ] ;


    # AGN_S17, AG, AI, AGN_HEC Measurements, CLASS_SP
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementAGN_S17> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementAGN_HEC> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementAG> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementAI> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementCLASS_SP> ] ] ;

    # UT, BT, VT, IT Measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementUT> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementBT> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementVT> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementIT> ] ] ;

    # Photometric Bands: U, G, R, T, I, Z
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementU> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementG> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementR> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementT> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementI> ] ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementZ> ] ] ;

    #Additional measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementJ> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementH> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementK> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementFLAG_SFR_HEC> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementINCL> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementV> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementV_VIR> ] ; ] ;


    #S band measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementS12> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementS25> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementS60> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementS100> ] ; ] ;
    

    #WF apparent magnitude measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementWF1> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementWF2> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementWF3> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementWF4> ] ; ] ;
        

    #WFPOINT AND WFTREAT measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementWFPOINT> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementWFTREAT> ] ; ] ;
    
    #logL Decimal logarithms in solar luminosities measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogL_12u> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogL_22u> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogL_60u> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogL_K> ] ; ] ;
    
    #ML_ratio measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementML_ratio> ] ; ] ;

    #Decimal logarithm of star-formation rate in solar masses per year measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogSFR_12u> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogSFR_22u> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogSFR_60u> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogSFR_HEC> ] ; ] ;
    
    #Decimal logarithm of stellar mass and SED-based formation rate measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogM_HEC> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogSFR_GSW> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogM_GSW> ] ; ] ;

    # Minimum signal-to-noise ratio and metallicity measurements
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementMIN_SNR> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementMETAL> ] ; ] ;

    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogL_TIR> ] ; ] ;


    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogL_FIR> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogSFR_TIR> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementlogSFR_FIR> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementNDIST> ] ; ] ;
    rr:predicateObjectMap [ rr:predicate parsec:P1_has_measurement ; rr:objectMap [ rr:parentTriplesMap <#MeasurementPA> ] ; ] ;


    .





################################################################################
# Identifier Mapping: Now includes ID_NED and ID_IRAS
################################################################################
<#IdentifierID_2MASS> a rr:TriplesMap ;
    rml:logicalSource <#HecateCSV> ;
    
    # Create a unique identifier resource per galaxy using the PGC.
    rr:subjectMap [
        rr:template "https://hecate.ia.forth.gr/E17_Identifier/ID_2MASS/galaxy_{pgc}" ;
        rr:class parsec:E17_Identifier ;
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate parsec:P43_identifier_value ;
        rr:objectMap [
            rr:template "{ID_2MASS}" ;
            rr:termType rr:Literal ;
        ] ;
    ] .

