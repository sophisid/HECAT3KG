<#GalaxyIdentifier> a rr:TriplesMap ;
    rml:logicalSource <#HecateCSV> ;
    
    # Create a unique identifier resource per galaxy using the PGC.
    rr:subjectMap [
        rr:template "https://hecate.ia.forth.gr/galaxy_identifier/{PGC}" ;
        rr:class parsec:E17_Identifier ;
    ] ;
    # P12 Has Identifier
    rr:predicateObjectMap [
        rr:predicate parsec:P12_has_identifier ;
        rr:objectMap [
            rr:template "{ID_2MASS}" ;
            rr:termType rr:Literal ;
        ] ;
    ] ;
    # P12 Has Identifier
    rr:predicateObjectMap [
        rr:predicate parsec:P12_has_identifier ;
        rr:objectMap [
            rr:template "{OBJNAME}" ;
            rr:termType rr:Literal ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P12_has_identifier ;
        rr:objectMap [
            rr:template "{PGC}" ;
            rr:datatype xsd:int ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P12_has_identifier ;
        rr:objectMap [
            rr:template "{ID_NED}" ;
            rr:termType rr:Literal ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P12_has_identifier ;
        rr:objectMap [
            rr:template "{sdss_photid}" ;
            rr:termType rr:Literal ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P12_has_identifier ;
        rr:objectMap [
            rr:template "{sdss_specid}" ;
            rr:termType rr:Literal ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P12_has_identifier ;
        rr:objectMap [
            rr:template "{ID_IRAS}" ;
            rr:termType rr:Literal ;
        ] ;
    ] ;

    # P44 Stores Identifiers as Literals (NO encoding)
    rr:predicateObjectMap [
        rr:predicate parsec:P44_is_identified_by ;
        rr:objectMap [
            rml:reference "ID_2MASS" ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P44_is_identified_by ;
        rr:objectMap [
            rml:reference "OBJNAME" ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P44_is_identified_by ;
        rr:objectMap [
            rml:reference "PGC" ;
            rr:datatype xsd:int ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P44_is_identified_by ;
        rr:objectMap [
            rml:reference "ID_NED" ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P44_is_identified_by ;
        rr:objectMap [
            rml:reference "sdss_photid" ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P44_is_identified_by ;
        rr:objectMap [
            rml:reference "sdss_specid" ;
        ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P44_is_identified_by ;
        rr:objectMap [
            rml:reference "ID_IRAS" ;
        ] ;
    ] .
