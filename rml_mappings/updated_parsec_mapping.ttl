################################################################################
# Measurement Mapping for Right Ascension (RA)
################################################################################
<#MeasurementRA> a rr:TriplesMap ;
    rml:logicalSource <#HecateCSV> ;
    
    # Create a unique URI for the RA measurement using the galaxy's PGC.
    rr:subjectMap [
        rr:template "https://parsec.forth.gr/celestial_body/galaxy_{PGC}/column/RA/measurement/galaxy_{PGC}_RA" ;
        rr:class parsec:E8_Measurement ;                      # Measurement entity type
    ] ;
    # Map the RA value from the CSV (as a float).
    rr:predicateObjectMap [
        rr:predicate parsec:P02_has_value ;
        rr:objectMap [ rml:reference "RA" ; rr:datatype xsd:float ] ;
    ] ;
    # Link this measurement to the RA column entity.
    rr:predicateObjectMap [
        rr:predicate parsec:P21_is_measurement_from_column ;
        rr:objectMap [ rr:parentTriplesMap <#ColumnRA> ] ;
    ] ;
        rr:predicateObjectMap [
        rr:predicate parsec:P05_has_lower_margin_value ;
        rr:objectMap [ rml:reference "E_RA" ; rr:datatype xsd:float ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate parsec:P06_has_upper_margin_value ;
        rr:objectMap [ rml:reference "E_RA" ; rr:datatype xsd:float ] ;
    ] .

################################################################################
# Measurement Mapping for Declination (DEC)
################################################################################
<#MeasurementDEC> a rr:TriplesMap ;
    rml:logicalSource <#HecateCSV> ;

    rr:subjectMap [
        rr:template "https://parsec.forth.gr/celestial_body/galaxy_{PGC}/column/DEC/measurement/galaxy_{PGC}_DEC" ;
        rr:class parsec:E8_Measurement ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate parsec:P02_has_value ;
        rr:objectMap [ rml:reference "DEC" ; rr:datatype xsd:float ] ;
    ] ;
        rr:predicateObjectMap [
        rr:predicate parsec:P21_is_measurement_from_column ;
        rr:objectMap [ rr:parentTriplesMap <#ColumnDEC> ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P05_has_lower_margin_value ;
        rr:objectMap [ rml:reference "E_DEC" ; rr:datatype xsd:float ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate parsec:P06_has_upper_margin_value ;
        rr:objectMap [ rml:reference "E_DEC" ; rr:datatype xsd:float ] ;
    ] .


################################################################################
# D Mapping 
################################################################################
<#MeasurementD> a rr:TriplesMap ;
    rml:logicalSource <#HecateCSV> ;
    
    rr:subjectMap [
        rr:template "https://parsec.forth.gr/celestial_body/galaxy_{PGC}/column/D/measurement/galaxy_{PGC}_D" ;
        rr:class parsec:E08_Measurement ;
    ] ;
    
    # D Value
    rr:predicateObjectMap [
        rr:predicate parsec:P02_has_value ;
        rr:objectMap [
            rml:reference "D" ;
            rr:datatype xsd:float ;
        ] ;
    ] ;

    # Links to D column definition
    rr:predicateObjectMap [
        rr:predicate parsec:P21_is_measurement_from_column ;
        rr:objectMap [
            rr:constant parsec:HECATE_D ;
        ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate parsec:P20_has_estimation_method ; 
        rr:objectMap [ rr:template "{D_Estimation_Method}" ; 
        rr:termType rr:Literal ] 
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate parsec:P26_has_error_confidence_interval_percentage ;
        rr:objectMap [ rr:constant "68" ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P26_has_error_confidence_interval_percentage ;
        rr:objectMap [ rr:constant "95" ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate parsec:P27_has_CI_lower_bound ;
        rr:objectMap [ rml:reference "D_LO68" ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P27_has_CI_lower_bound ;
        rr:objectMap [ rml:reference "D_LO95" ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate parsec:P28_has_CI_upper_bound ;
        rr:objectMap [ rml:reference "D_HI68" ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate parsec:P28_has_CI_upper_bound ;
        rr:objectMap [ rml:reference "D_HI95" ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate parsec:P05_has_lower_margin_value ;
        rr:objectMap [ rml:reference "E_D" ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate parsec:P06_has_upper_margin_value ;
        rr:objectMap [ rml:reference "E_D" ] ;
    ] .


################################################################################
# AGN_S17 Measurement Mapping
