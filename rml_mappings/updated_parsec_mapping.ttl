################################################################################
# Position Angle (PA) Mapping with Reliability Status
################################################################################
<#MeasurementPA> a rr:TriplesMap ;
    rml:logicalSource <#HecateCSV> ;
    
    # Create a unique resource for the PA measurement for each galaxy
    rr:subjectMap [
        rr:template "https://parsec.forth.gr/celestial_body/galaxy_{PGC}/column/PA/measurement/galaxy_{PGC}_PA" ;
        rr:class parsec:E08_Measurement ;
    ] ;
    
    # PA Value
    rr:predicateObjectMap [
        rr:predicate parsec:P02_has_value ;
        rr:objectMap [
            rml:reference "PA" ;
            rr:datatype xsd:float ;
        ] ;
    ] ;

    # Links to PA column definition
    rr:predicateObjectMap [
        rr:predicate parsec:P21_is_measurement_from_column ;
        rr:objectMap [
            rr:constant parsec:HECATE_PA ;
        ] ;
    ] ;

    # Reliability Status Mapping
    rr:predicateObjectMap [
    rr:predicate parsec:P29_has_reliability_status ;
    rr:objectMap [
        rml:reference "Reliability_Status" ;
    ] ;
    ] ;


    # Data Source (Calculated from preprocessing, it is static HyperLEDA)
    rr:predicateObjectMap [
        rr:predicate parsec:P46_originates_from_data_source ;
        rr:objectMap [
            rml:reference "Source_Label" ;
        ] ;
    ] .


