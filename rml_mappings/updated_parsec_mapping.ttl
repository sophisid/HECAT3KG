    # Link to the AGN_S17 column entity
    rr:predicateObjectMap [
        rr:predicate parsec:P21_is_measurement_from_column ;
        rr:objectMap [ rr:parentTriplesMap <#ColumnAGN_S17> ] ;
    ] .



