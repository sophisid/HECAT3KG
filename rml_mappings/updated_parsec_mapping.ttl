################################################################################
# NDIST Mapping with Reliability Status
################################################################################
<#MeasurementNDIST> a rr:TriplesMap ;
    rml:logicalSource <#HecateCSV> ;
    
    rr:subjectMap [
        rr:template "https://parsec.forth.gr/celestial_body/galaxy_{PGC}/column/NDIST/measurement/galaxy_{PGC}_NDIST" ;
        rr:class parsec:E08_Measurement ;
    ] ;
    
    # NDIST Value
    rr:predicateObjectMap [
        rr:predicate parsec:P02_has_value ;
        rr:objectMap [
            rml:reference "NDIST" ;
            rr:datatype xsd:float ;
        ] ;
    ] ;

    # Links to NDIST column definition
    rr:predicateObjectMap [
        rr:predicate parsec:P21_is_measurement_from_column ;
        rr:objectMap [
            rr:constant parsec:HECATE_NDIST ;
        ] ;
    ] ;
    rr:predicateObjectMap [
    rr:predicate parsec:P29_has_reliability_status ; 
        rr:objectMap [ rr:template "{EDIST}" ; 
        rr:termType rr:Literal ] 
    ] .







